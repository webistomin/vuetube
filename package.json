{
  "name": "vuetube",
  "version": "1.0.1",
  "author": {
    "name": "Alexey Istomin",
    "email": "webistomin@gmail.com",
    "url": "https://github.com/webistomin"
  },
  "description": "A fast, lightweight, lazyload vue component acting as a thin layer over the YouTube IFrame Player API which renders fast",
  "keywords": [
    "youtube",
    "youtube-iframe",
    "youtube-iframe-player-api",
    "youtube-player",
    "youtube-embed",
    "vue-player",
    "vue-youtube"
  ],
  "license": "MIT",
  "main": "dist/vuetube.cjs.js",
  "browser": "dist/vuetube.esm.js",
  "module": "dist/esm/vuetube.js",
  "exports": {
    ".": {
      "browser": "./dist/vuetube.js",
      "require": "./dist/vuetube.cjs.js",
      "import": "./dist/esm/vuetube.js",
      "default": "./dist/esm/vuetube.js"
    },
    "./": "./",
    "./package.json": "./package.json"
  },
  "unpkg": "dist/vuetube.iife.js",
  "jsdelivr": "dist/vuetube.iife.js",
  "types": "dist/types/vuetube.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "reveal": true,
  "sideEffects": [
    ".css"
  ],
  "scripts": {
    "dev": "start-storybook -p 3000",
    "prebuild": "rimraf dist",
    "build": "run-s prebuild build:all build:typings build:css",
    "build:all": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:esm": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format esm",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:iife": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:css": "cross-env NODE_ENV=production rollup --config build/rollup.css.config.js",
    "build:size": "run-s prebuild build check-size",
    "build:typings": "run-s build:declaration",
    "build:declaration": "tsc -p tsconfig.package.json && tsc-alias -p tsconfig.alias.json",
    "build:storybook": "build-storybook",
    "lint": "run-s eslint:write ls:lint",
    "eslint:write": "eslint --ext .js,.vue,.ts --ignore-path .gitignore . --max-warnings 0",
    "ls:lint": "npx @ls-lint/ls-lint",
    "test": "vue-cli-service test:unit",
    "tree-shake": "agadoo",
    "check-size": "size-limit",
    "release": "dotenv release-it --verbose"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "dependencies": {
    "@types/youtube": "0.0.42"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-angular": "^12.0.1",
    "@ls-lint/ls-lint": "^1.9.2",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-a11y": "^6.1.20",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/theming": "^6.1.20",
    "@storybook/vue": "^6.1.20",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.3",
    "agadoo": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash.findindex": "^4.6.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "release-it": "^14.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "size-limit": "^4.9.2",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.5.4",
    "tsc-alias": "^1.2.7",
    "typescript": "^4.2.2",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">= 14.15.4",
    "npm": ">= 6.14.7"
  },
  "bugs": {
    "url": "https://github.com/webistomin/vuetube/issues"
  },
  "homepage": "https://github.com/webistomin/vuetube#readme",
  "repository": {
    "type": "git",
    "url": "githttps://github.com/webistomin/vuetube.git"
  }
}
